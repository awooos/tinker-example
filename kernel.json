{
  "os_name": "SomeExampleOS",
  "default_target": "i386",
  "dependencies": [
    {
      "name": "flail",
      "origin": "https://github.com/awooos/flail.git#master",
      "initializeArgs": ["\"tinker-generated kernel\"", "&hal_putchar"],
      "run": false
    },
    {
      "name": "dmm",
      "origin": "https://github.com/awooos/dmm.git#master",
      "initializeArgs": ["&_flail_panic"],
      "run": false
    },
    {
      "name": "ali",
      "origin": "https://github.com/awooos/ali.git#master",
      "initializeArgs": ["&dmm_malloc_", "&dmm_free_", "&dmm_realloc_"],
      "run": false
    },
    {
      "name": "tinker",
      "origin": "https://github.com/awooos/tinker.git#main",
      "initialize": false,
      "run": false
    },
    {
      "name": "hal",
      "origin": "./libs/hal",
      "initializeArgs": [],
      "run": false
    }
  ]
}
